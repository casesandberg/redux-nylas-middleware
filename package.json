{
  "name": "redux-nylas-middleware",
  "version": "0.1.4",
  "description": "Redux middleware for calling the Nylas API",
  "main": "index.js",
  "scripts": {
    "test": "npm run unit-test && npm run eslint",
    "unit-test": "jest",
    "lib": "node_modules/.bin/babel src -d .",
    "prepublish": "npm run lib",
    "eslint": "node_modules/.bin/eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/casesandberg/redux-nylas-middleware.git"
  },
  "keywords": [
    "redux",
    "middleware",
    "nylas",
    "api"
  ],
  "author": "case <case@casesandberg.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/casesandberg/redux-nylas-middleware/issues"
  },
  "homepage": "https://github.com/casesandberg/redux-nylas-middleware#readme",
  "jest": {
    "setupFiles": [
      "__tests__/__mocs__/localStorage.js",
      "__tests__/__mocs__/fetch.js"
    ],
    "testRegex": "spec.js$"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^11.2.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "jest": "^16.0.2",
    "jest-fetch-mock": "^1.0.6",
    "redux": "^3.6.0",
    "redux-mock-store": "^1.2.1"
  }
}
